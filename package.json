{
  "name": "maxmilhas-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "ttsc -p tsconfig.build.json",
    "migration:run": "prisma migrate deploy",
    "migration:dev": "dotenv -e .env.development prisma migrate dev",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "start": "node dist/server.js",
    "start:docker": "docker-compose up",
    "dev:docker": "docker-compose -f docker-compose.development.yml --env-file .env.development up",
    "test": "dotenv -e .env.test jest --no-cache",
    "test:unit": "dotenv -e .env.test jest -- -i --coverage",
    "test:migration": "dotenv -e .env.test npx prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.7.1"
  }
